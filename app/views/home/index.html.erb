      <!-- CONTACT -->
      <div id="contact" class="section contact padding-top padding-bottom">
        <div class="section-heading">
          <h2>CONTACT</h2>
          <hr />
        </div>
        <div class="section-content">
          <div class="container">
            <div class="row">
              <div class="col-md-12 col-sm-12 col-xs-12 text-center">
                <div class="contact-info">
                  <div class="col-md-4 col-sm-4 col-xs-12">
                    <i class="icon ion-map"></i>
                    <h4>ADDRESS</h4>
                    <p><%= t('.adress') %></p>
                  </div>
                  <div class="col-md-4 col-sm-4 col-xs-12">
                    <i class="icon ion-ios-telephone"></i>
                    <h4>PHONE</h4>
                    <p>+82 70 8679 2503</p>
                  </div>
                  <div class="col-md-4 col-sm-4 col-xs-12">
                    <i class="icon ion-ios-email"></i>
                    <h4>EMAIL</h4>
                    <p><a href="mailto:rr5ys5s@gmail.com">support@tagsolution.kr</a></p>
                  </div>
                </div>
              </div>
              <div class="col-md-12 col-sm-12 col-xs-12 text-center padding-top">
                <div class="contact-form">
                  <div class="alert"></div>
                  <form id="contact-form" action="home/submit" method="post">
                    <div class="row">
                      <div class="col-md-6 col-sm-6 col-xs-6">
                        <label class="sr-only" for="name">Name:</label>
                        <input type="text" id="name" name="name" placeholder="Name *" required>
                      </div>
                      <div class="col-md-6 col-sm-6 col-xs-6">
                        <label class="sr-only" for="email">Email:</label>
                        <input type="email" id="email" name="email" placeholder="Only for Email with '@'" required>
                      </div>
                    </div>
                    <div class="row text-right">
                      <div class="col-md-12 col-sm-12 col-xs-12">
                        <label class="sr-only" for="message">Message:</label>
                        <textarea id="message" name="message" placeholder="Message *" required></textarea>
                        <button onclick="alert('Complete')" type="submit" class="btn btn-primary btn-lg" name="send">SEND</button>
                      </div>
                    </div>
                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
